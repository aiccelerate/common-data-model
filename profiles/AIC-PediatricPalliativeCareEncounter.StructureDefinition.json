  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Encounter.type",
        "path": "Encounter.type",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "coding.system"
            }
          ],
          "rules": "open"
        }
      },
      {
        "id": "Encounter.type:aicEncounterType",
        "path": "Encounter.type",
